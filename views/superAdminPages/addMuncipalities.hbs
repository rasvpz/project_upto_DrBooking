		<form action="/superAdmin/addMuncipality" method="POST">  

        			<select name="district" id="distValue" onchange="random_function()">
					   <option name="" value="">Select District</option>
					   {{#each dist}}
						<option name="{{this.district}}" value="{{this.district}}">{{this.district}}</option>
						{{/each}}
						</select>      			


					<select  name="muncipality" id="output" onchange="random_function1()">
						<option value="">Select Muncipality</option>
					</select>
			
		  <input type="submit" value="ADD">&nbsp;&nbsp;&nbsp;
          <a href="/superAdmin" type="button">Close</button>
        </form>
		<table>
			{{#each munc}}
			<tr>
				<td>{{this.myMuncipalityId}}</td> <td>{{this.muncipality}}</td>  <td><a onclick="return confirm('Are you sure, you want to delete it?')" href="/superAdmin/deleteMuncipalites/{{this._id}}">delete</a></td>
			</tr>
			{{/each}}
		</table>
